titleLabel.TextSize = 20
titleLabel.Font = Enum.Font.Merriweather 
titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)

local infJumpEnabled = false
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

player.CharacterAdded:Connect(function(char)
    character = char
    humanoid = char:WaitForChild("Humanoid")
end)

game:GetService("UserInputService").JumpRequest:Connect(function()
    if infJumpEnabled and humanoid then
        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

tabgodmode:AddSwitch("Inf Jump", function(state)
    infJumpEnabled = state
end)

local switch = tabgodmode:AddSwitch("Disable Trade", function(State)
            if State then
                game:GetService("ReplicatedStorage").rEvents.tradingEvent:FireServer("disableTrading")
            else
                game:GetService("ReplicatedStorage").rEvents.tradingEvent:FireServer("enableTrading")
            end
        end)
switch:Set(true)

local showpetsswitch = tabgodmode:AddSwitch("Show Pets", function(bool)
    local player = game:GetService("Players").LocalPlayer
    if player:FindFirstChild("hidePets") then
        player.hidePets.Value = bool
    end
end)
showpetsswitch:Set(false)

local showotherpetsswitch = tabgodmode:AddSwitch("Show Other Pets", function(bool)
    local player = game:GetService("Players").LocalPlayer
    if player:FindFirstChild("showOtherPetsOn") then
        player.showOtherPetsOn.Value = bool
    end
end)
showotherpetsswitch:Set(false)

local chatSpyEnabled = false
local spyConnection = nil

tabgodmode:AddSwitch("Chat Spy", function(state)
    chatSpyEnabled = state
    
    if chatSpyEnabled then
        local chatEvents = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents")
        local sayMessage = chatEvents:WaitForChild("SayMessageRequest")
        local onMessageDone = chatEvents:WaitForChild("OnMessageDoneFiltering")
        
        local function processMessage(speaker, message)
            if speaker == player then return end
            
            local cleanMsg = message:gsub("[\n\r]", ""):gsub("\t", " "):gsub("[ ]+", " ")
            local isPublic = false
            
            local conn = onMessageDone.OnClientEvent:Connect(function(msgData, channel)
                if msgData.SpeakerUserId == speaker.UserId then
                    isPublic = true
                end
            end)
            
            wait(0.5)
            conn:Disconnect()
            
            if not isPublic then
                game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
                    Text = "[SPY] " .. speaker.Name .. ": " .. cleanMsg,
                    Color = Color3.fromRGB(0, 255, 255)
                })
            end
        end
        
        for _, p in pairs(game.Players:GetPlayers()) do
            if p ~= player then
                p.Chatted:Connect(function(msg)
                    processMessage(p, msg)
                end)
            end
        end
        
        game.Players.PlayerAdded:Connect(function(newPlayer)
            newPlayer.Chatted:Connect(function(msg)
                processMessage(newPlayer, msg)
            end)
        end)
       
    else
    end
end)

tabgodmode:AddButton("Korblox", function()
    local char = player.Character
    if char then
        local rLower = char:FindFirstChild("RightLowerLeg")
        local rUpper = char:FindFirstChild("RightUpperLeg")
        local rFoot = char:FindFirstChild("RightFoot")
        
        if rLower and rUpper and rFoot then
            rLower.MeshId = "http://www.roblox.com/asset/?id=902942093"
            rLower.Transparency = 1
            rUpper.MeshId = "http://www.roblox.com/asset/?id=902942096"
            rUpper.TextureID = "http://roblox.com/asset/?id=902843398"
            rFoot.MeshId = "http://www.roblox.com/asset/?id=902942089"
            rFoot.Transparency = 1
           
        end
    end
end)

